#include<stdio.h>


void swap(int* x,int* y)
{
	int temp;
	temp = *x;
	*x = *y;
	*y = temp;
}

void InsertSort(int arr[],int n)
{
	int i,j;
	int h;
	
	for(i=1;i<n;i++)
	{
	   h=i;
	   for(j=0;j<n;j++)
	   {
	   	   if(arr[j]>arr[h])
		   	  {
	   	   	     swap(&arr[j],&arr[h]);
			  }
		   
		}	
		
	}
}

void dis(int arr[],int n)
{
	int a;
	printf("\nSorted Array: ");
	for(a=0;a<n;a++)
	{
		printf("%d ",arr[a]);
	}
}


int main()
{
	int arr[] ={34,8,64,51,32,21};
	int n = sizeof(arr)/sizeof(arr[0]);
	
	InsertSort(arr,n); 
	
	dis(arr,n);
}
